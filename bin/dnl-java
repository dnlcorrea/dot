#!/usr/bin/env python

import sys, re
from string import Template

# + String
# . int

props = Template("""private $type $name;

public $type get$name() {return this.$name;}

public void set$name($type $name) {this.$name = $name;}
""")

string = sys.argv[1]

for i in re.findall('\+\w+', string):
    print(props.substitute(name=i.strip('+'), type='String'))


print()

for i in re.findall('\.\w+', string):
    print(props.substitute(name=i.strip('.'), type='int'))
