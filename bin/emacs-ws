#!/usr/bin/env fish

set current (emacsclient -e '(format "%i" exwm-workspace-current-index)' | sed 's/"//g')

set total (emacsclient -e '(- (exwm-workspace--count) 1)')

echo $total

for i in (seq 0 $total)
    if test $i = $current
        echo -n "[$i]  "
    else
        echo -n "$i  "
    end
end
