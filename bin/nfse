    IDEMPCONTHOMO.DTEXECUCAOINI=\
    IDEMPCONTHOMO.DTEXECUCAOFIM=\
    IDEMPCONTHOMO.VLSERVICO=\
    IDEMPCONTHOMO._IDEMPCONT.STUNIDADEECONOMICA=\
    STPRE_CPFCNPJ=32987680000175\
    STPRE_STIM=76431\
    idEmpresa=23928\
    stTom_Pessoa_Tipo=F\
    stTom_CpfCnpj=41892852934\
    stTom_CpfCnpj_Estrangeiro=41892852934\
    stTom_Nome=Julianne Fischer\
    stTom_CEP=85856750\
    stTom_End_Logr=Av. Felipe Wandscheer\
    stTom_End_Numero=6611\
    stTom_End_Bairro=Cognopolis\
    stTom_End_Complemento=casa 87\
    stTom_Telefone=\
    stTom_NomeCidade=Foz do Iguaçu\
    stTom_UF=PR\
    stCodigoIBGE=4108304\
    stTom_IM=\
    stTom_Email=\
    stEnviaPorEmail=true\
    stServico_Descricao=Desenvolvimento de Website\
